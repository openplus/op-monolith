        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/default_content_deploy": "dev-2.1.x",
        "drupal/restui": "^1.21",
        "drupaloverride/better_normalizers": "dev-master",
        "drupal/moderation_sidebar": "^1.7",
        "drupal/bg_image_formatter": "^1.16",
        "drupal/simple_gmap": "^3.0",
