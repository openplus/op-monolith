        "drupal/environment_indicator": "^4.0",
        "drupal/shield": "^1.7",
        "drupal/rdf": "^2.1",
        "drupal/quickedit": "^1.0",
        "drupal/migrate_source_csv": "^3.5",
        "drupal/unique_content_field_validation": "^1.1",
        "drupal/file_entity": "2.0-rc6 as 2.0-beta10",
