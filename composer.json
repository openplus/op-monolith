        "symfony/http-client": "^6.4",
        "drupal/ckeditor_find": "^2.0@beta",
        "drupal/insert_view_adv": "^2.0@alpha"
    },
    "require-dev": {
        "drupal/core-dev": "^10.2",
        "drupal/drupal-extension": "5.0.0-alpha1",
        "drush/drush": "^12.4.3",
        "oomphinc/composer-installers-extender": "^1.1 || ^2",
        "squizlabs/php_codesniffer": "3.*",
        "phpspec/prophecy-phpunit": "^2",
        "vlucas/phpdotenv": "^5.1",
        "webflo/drupal-finder": "^1.2"
    },
    "config": {
        "secure-http": false,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "wikimedia/composer-merge-plugin": true,
            "php-http/discovery": true
        }
    },
    "allow-plugins": {
        "composer/installers": true,
        "cweagans/composer-patches": true,
        "drupal/core-composer-scaffold": true,
        "oomphinc/composer-installers-extender": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "composer",
            "url": "https://drupalwxt.github.io/composer-extdeps/"
        },
        {
            "type": "package",
            "package": {
                "name": "drupaloverride/better_normalizers",
                "version": "dev-master",
                "type": "drupal-module",
                "dist": {
                    "url": "https://git.drupalcode.org/api/v4/projects/project%2Fbetter_normalizers/repository/archive.zip?sha=312e16",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/find",
                "version": "4.14.1",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "ckeditor/plugins/find"
                },
                "dist": {
                    "url": "https://download.ckeditor.com/find/releases/find_4.14.1.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "furf/jquery-ui-touch-punch",
                "version": "v0.2.3",
                "type": "drupal-library",
                "dist": {
                    "url": "https://raw.githubusercontent.com/furf/jquery-ui-touch-punch/master/jquery.ui.touch-punch.min.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
              "name": "nnnick/chartjs",
              "version": "v3.9.1",
              "type": "drupal-library",
              "dist": {
                "url": "https://github.com/chartjs/Chart.js/releases/download/v3.9.1/chart.js-3.9.1.tgz",
                "type": "tar"
              }
            }
        }
    ],
    "scripts": {
        "post-create-project-cmd": "rm -r -f .travis.yml behat.yml .travis-ci",
        "quick-start": [
            "composer install"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "html/"
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset",
            "drupal-custom-module",
            "drupal-custom-feature"
        ],
        "installer-paths": {
            "html/core": [
                "type:drupal-core"
            ],
            "html/libraries/{$name}": [
                "type:drupal-console-library",
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "html/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "html/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "html/modules/features/{$name}": [
                "type:drupal-custom-feature"
            ],
            "html/profiles/{$name}": [
                "type:drupal-profile"
            ],
            "html/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "composer-exit-on-patch-failure": true,
        "enable-patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "2652000 - Views do not add the active class anymore": "https://www.drupal.org/files/issues/2020-01-09/2652000-84.patch",
                "2885278 - Node local tasks tabs do not appear on node revisions": "https://www.drupal.org/files/issues/2019-11-12/drupal-node_revision_local_tasks_canonical-2885278-15.patch",
                "2918537 - Cannot save unpublished versions of published content for users without manage book privileges": "https://www.drupal.org/files/issues/2020-10-18/2918537-63.patch",
                "2918784 - Views RSS row plugin incorrectly assumes links are relative": "https://www.drupal.org/files/issues/2020-02-19/2918784-28-allow-external-url-for-rss-link.patch",
                "2928108 - Call to a member function isTranslatable() on null in SqlContentEntityStorage": "https://www.drupal.org/files/issues/2019-12-05/2928108-49.patch",
                "3093800 - Argument default about revision id from url": "https://www.drupal.org/files/issues/2020-12-24/3093800-11.patch",
                "2429699 - Add Views EntityReference filter to be available for all entity reference fields": "https://git.drupalcode.org/project/drupal/-/merge_requests/9226.diff",
                "3150294 - New translations for moderated nodes are not created in the initial workflow state": "https://www.drupal.org/files/issues/2022-01-05/3150294-95.patch",
                "3264979 - Deprecated function: Implicit conversion from float to int": "https://www.drupal.org/files/issues/2022-12-22/3264979-16.patch",
                "2840283 - Problem with action path for embedded forms": "https://git.drupalcode.org/project/drupal/-/merge_requests/8053.diff",
                "3161942 - URL Alias: Extend filter aliases to recognize system path": "https://www.drupal.org/files/issues/2020-07-28/3161942-2-filter-aliases-to-search-through-alias-or-system-path.patch",
                "2313309 - Admin toolbar follow lang preference": "https://www.drupal.org/files/issues/2023-12-19/2313309-179.patch",
                "2783897 - Select all for theme visibility condition": "https://www.drupal.org/files/issues/2023-07-10/core_drupal-current-theme_2783897_11x_37_0.patch",
                "3417971 - wsod when saving content language settings": "https://www.drupal.org/files/issues/2024-09-19/logic_exception_wsod_temporary-3417971-14.patch"
            },
            "drupal/entityqueue": {
                "3124403 - Type error when subqueue creation fails": "https://www.drupal.org/files/issues/2020-04-02/type-error-3124403-2.patch"
            },
            "drupal/file_entity": {
                "3157682 - Make field settings translatable": "https://www.drupal.org/files/issues/2020-07-08/3157682.patch"
            },
            "drupal/pathauto": {
                "2973478 - Aliases are not created for affected translations": "https://www.drupal.org/files/issues/2018-07-12/2973478-4.patch"
            },
            "drupal/taxonomy_manager": {
                "3046752 - AJAX error when editing term content": "https://www.drupal.org/files/issues/2019-07-30/taxonomy_manager-3046752-1.patch"
            },
            "drupal/conditional_fields": {
                "3192044 - Notice: Trying to access array offset on value of type null in": "https://www.drupal.org/files/issues/2021-01-14/fix-notice-3192044-14.patch"
            },
            "drupal/tac_lite": {
                "Add the ability to include all taxonomy terms without having to select them": "https://www.drupal.org/files/issues/2021-03-15/tac_lite-add_all_option-3203718-2.patch"
            },
            "drupal/backup_migrate": {
                "3245802 Fix restore of public files": "https://www.drupal.org/files/issues/2022-01-18/backup-migrate-restore-public-files-3245802-3.patch"
            },
            "drupal/layout_builder_st": {
                "3069964 - Fix isTranslation error": "https://www.drupal.org/files/issues/2019-07-24/isTranslation-null-given.patch"
            },
            "drupal/redirect": {
                "2981544 - Filter by redirect destination does not work with URL alias": "https://www.drupal.org/files/issues/2024-08-13/2981544-45.patch"
            },
            "drupal/linkit": {
                "Detect and strip base URL from pasted URLs to increase matching hits": "https://www.drupal.org/files/issues/2023-11-02/linkit6x-3078075-77.patch"
            },
            "drupal/wxt_bootstrap": {
                "3400277": "https://www.drupal.org/files/issues/2023-11-08/wxt-bootstrap-byline-3400277-3.patch"
            },
            "drupal/rabbit_hole": {
                "3383171 - Call to undefined method Drupal/entity_browser/Form/EntityBrowserForm::getEntity()": "https://www.drupal.org/files/issues/2023-09-15/3383171-7.patch",
                "3400907 - WSOD on deleting translation": "https://www.drupal.org/files/issues/2024-08-19/3400907-11-10.3.patch"
            },
            "drupal/workflow": {
                "3414448 - Call to undefined method Drupal/entity_browser/Form/EntityBrowserForm::getEntity()": "https://www.drupal.org/files/issues/2024-01-12/3414448-2-WorkflowDefaultWidget.patch"
            },
            "drupal/administration_language_negotiation": {
                "3366786 - Deprecated function: Creation of dynamic property $cacheKeyPrefix": "https://www.drupal.org/files/issues/2023-07-12/deprecated-function-creation-of-dynamic-property-3366786.patch"
            },
            "drupal/entity_usage": {
               "3015287 - Use Drupal queue for updating the usage of entities": "https://www.drupal.org/files/issues/2024-02-09/3015287-entity-usage-tracker-queue.patch"
            },
            "drupal/yearonly": {
                "3145858 - Reverse order of year selection": "https://www.drupal.org/files/issues/2022-12-13/yearonly-reverse_order-3145858-15.patch",
                "3035638 - Allow add extra years (NOW + x)": "https://www.drupal.org/files/issues/2019-02-25/yearonly-allow_extra%2Byears-3035638-2-D8.patch"
            }
        },
        "patches-ignore": { }
    },
    "autoload": {
        "classmap": [
            "scripts/ScriptHandler.php"
        ]
    }
}
