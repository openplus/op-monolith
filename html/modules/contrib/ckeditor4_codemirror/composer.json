{
  "name": "drupal/ckeditor4_codemirror",
  "description": "Adds the CKEditor4 CodeMirror source syntax highlighter plugin.",
  "type": "drupal-module",
  "keywords": ["Drupal"],
  "license": "GPL-2.0-or-later",
  "homepage": "https://drupal.org/project/ckeditor4_codemirror",
  "support": {
    "issues": "https://www.drupal.org/project/issues/ckeditor4_codemirror?version=8.x",
    "source": "https://git.drupalcode.org/project/ckeditor4_codemirror"
  },
  "authors": [
    {
      "name": "Joseph Olstad (joseph.olstad)",
      "homepage": "https://www.drupal.org/u/joseph.olstad",
      "role": "Maintainer"
    },
    {
      "name": "Nickolay Leshchev (Plazik)",
      "homepage": "https://www.drupal.org/u/plazik",
      "role": "Maintainer"
    },
    {
      "name": "Christopher C. Wells (wells)",
      "homepage": "https://www.drupal.org/u/wells",
      "role": "Co-maintainer"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "drupal/core": "^9.4 || ^10.0"
  },
  "require-dev": {
    "drupal/coder": "^8.3",
    "phpcompatibility/php-compatibility": "10.x-dev"
  },
  "minimum-stability": "dev",
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
